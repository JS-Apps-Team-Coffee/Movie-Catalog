<div class="container">
<div class="row">
    <table id="search-table" class="table table-striped table-hover">
      <thead>
        <tr>
          <th>#</th>
          <th>Title</th>
          <th>Year</th>
          <th>Type</th>
        </tr>
      </thead>
      <tbody>
      {{#each searchedMovies}}
        <tr>
          <td>{{@index}}</td>
          <td><span class="search-item">{{title}}</span></td>
          <td><span>{{year}}</span></td>
          <td><span>{{type}}</span></td>
        </tr>
        {{/each}}   
      </tbody>
    </table>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3" id="top-movie">
  </div>
</div>
</div>
